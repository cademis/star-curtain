    "lint:apps": "nx run-many --target=lint --projects=client,server",
    "lint:packages": "nx run-many --target=lint --projects=db,trpc-server",
    "clean": "nx reset",
    "graph": "nx graph",
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test",
    "affected:lint": "nx affected --target=lint"
