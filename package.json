{
  "devDependencies": {
    "@nx/eslint": "21.1.2",
    "@nx/js": "21.1.2",
    "@nx/jest": "21.1.2",
    "@nx/vite": "21.1.2",
    "@nx/web": "21.1.2",
    "@nx/webpack": "21.1.2",
    "@vitest/ui": "^3.0.0",
    "concurrently": "^9.1.2",
    "jiti": "2.4.2",
    "nx": "21.1.2",
    "typescript": "^5",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  },
  "type": "module",
  "scripts": {
    "dev": "nx run-many --target=serve --projects=client,server --parallel",
    "dev:apps": "nx run-many --target=serve --projects=client,server --parallel",
    "dev:packages": "nx run-many --target=dev --projects=db,trpc-server --parallel",
