#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chris/repos/star-curtain/node_modules/.pnpm/@antfu+eslint-config@4.11.0_@typescript-eslint+utils@8.28.0_@vue+compiler-sfc@3.5.13_eslint@9_7qed5277luay6vb6dxhrqpf7gy/node_modules/@antfu/eslint-config/bin/node_modules:/Users/chris/repos/star-curtain/node_modules/.pnpm/@antfu+eslint-config@4.11.0_@typescript-eslint+utils@8.28.0_@vue+compiler-sfc@3.5.13_eslint@9_7qed5277luay6vb6dxhrqpf7gy/node_modules/@antfu/eslint-config/node_modules:/Users/chris/repos/star-curtain/node_modules/.pnpm/@antfu+eslint-config@4.11.0_@typescript-eslint+utils@8.28.0_@vue+compiler-sfc@3.5.13_eslint@9_7qed5277luay6vb6dxhrqpf7gy/node_modules/@antfu/node_modules:/Users/chris/repos/star-curtain/node_modules/.pnpm/@antfu+eslint-config@4.11.0_@typescript-eslint+utils@8.28.0_@vue+compiler-sfc@3.5.13_eslint@9_7qed5277luay6vb6dxhrqpf7gy/node_modules:/Users/chris/repos/star-curtain/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chris/repos/star-curtain/node_modules/.pnpm/@antfu+eslint-config@4.11.0_@typescript-eslint+utils@8.28.0_@vue+compiler-sfc@3.5.13_eslint@9_7qed5277luay6vb6dxhrqpf7gy/node_modules/@antfu/eslint-config/bin/node_modules:/Users/chris/repos/star-curtain/node_modules/.pnpm/@antfu+eslint-config@4.11.0_@typescript-eslint+utils@8.28.0_@vue+compiler-sfc@3.5.13_eslint@9_7qed5277luay6vb6dxhrqpf7gy/node_modules/@antfu/eslint-config/node_modules:/Users/chris/repos/star-curtain/node_modules/.pnpm/@antfu+eslint-config@4.11.0_@typescript-eslint+utils@8.28.0_@vue+compiler-sfc@3.5.13_eslint@9_7qed5277luay6vb6dxhrqpf7gy/node_modules/@antfu/node_modules:/Users/chris/repos/star-curtain/node_modules/.pnpm/@antfu+eslint-config@4.11.0_@typescript-eslint+utils@8.28.0_@vue+compiler-sfc@3.5.13_eslint@9_7qed5277luay6vb6dxhrqpf7gy/node_modules:/Users/chris/repos/star-curtain/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
else
  exec node  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
fi
